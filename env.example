# OAuth Configuration
# Copy this file to .env and fill in your actual keys

# WeChat OAuth
REACT_APP_WECHAT_CLIENT_ID=your_wechat_client_id_here
REACT_APP_WECHAT_CLIENT_SECRET=your_wechat_client_secret_here

# Alipay OAuth
REACT_APP_ALIPAY_CLIENT_ID=your_alipay_client_id_here
REACT_APP_ALIPAY_CLIENT_SECRET=your_alipay_client_secret_here

# Google OAuth (if needed)
REACT_APP_GOOGLE_CLIENT_ID=your_google_client_id_here
REACT_APP_GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# Apple OAuth (if needed)
REACT_APP_APPLE_CLIENT_ID=your_apple_client_id_here
REACT_APP_APPLE_CLIENT_SECRET=your_apple_client_secret_here
